#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ankitanand/webui-integration/backend/efficio_backend_complete/node_modules/.pnpm/yamljs@0.3.0/node_modules/yamljs/bin/node_modules:/Users/ankitanand/webui-integration/backend/efficio_backend_complete/node_modules/.pnpm/yamljs@0.3.0/node_modules/yamljs/node_modules:/Users/ankitanand/webui-integration/backend/efficio_backend_complete/node_modules/.pnpm/yamljs@0.3.0/node_modules:/Users/ankitanand/webui-integration/backend/efficio_backend_complete/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ankitanand/webui-integration/backend/efficio_backend_complete/node_modules/.pnpm/yamljs@0.3.0/node_modules/yamljs/bin/node_modules:/Users/ankitanand/webui-integration/backend/efficio_backend_complete/node_modules/.pnpm/yamljs@0.3.0/node_modules/yamljs/node_modules:/Users/ankitanand/webui-integration/backend/efficio_backend_complete/node_modules/.pnpm/yamljs@0.3.0/node_modules:/Users/ankitanand/webui-integration/backend/efficio_backend_complete/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/yamljs@0.3.0/node_modules/yamljs/bin/yaml2json" "$@"
else
  exec node  "$basedir/../.pnpm/yamljs@0.3.0/node_modules/yamljs/bin/yaml2json" "$@"
fi
